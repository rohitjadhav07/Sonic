{
  "name": "astra-ai",
  "version": "1.0.0",
  "description": "AI-Powered Blockchain Agent for Sei Network",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python main.py",
    "dev:backend:demo": "cd backend && python main_demo.py",
    "build": "cd frontend && npm run build",
    "start": "cd frontend && npm start",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "deploy:contracts": "npx hardhat run scripts/deploy-subscription.js --network sonic-testnet",
    "deploy:mainnet": "npx hardhat run scripts/deploy-subscription.js --network sonic-mainnet",
    "test:demo": "node test-demo.js",
    "network:info": "node scripts/network-info.js",
    "network:test": "npx hardhat run scripts/test-network.js",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "verify:testnet": "npx hardhat verify --network sonic-testnet",
    "verify:mainnet": "npx hardhat verify --network sonic-mainnet"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": ["web3", "blockchain", "ai", "sei-network", "crypto", "wallet"],
  "authors": ["Astra AI Team"],
  "license": "MIT"
}